{
  "name": "cycles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:generate:dev": "prisma generate --schema=prisma/schema.sqlite.prisma",
    "db:push": "prisma db push",
    "db:push:dev": "prisma db push --schema=prisma/schema.sqlite.prisma",
    "db:migrate": "prisma migrate dev",
    "db:migrate:dev": "prisma migrate dev --schema=prisma/schema.sqlite.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:studio:dev": "prisma studio --schema=prisma/schema.sqlite.prisma",
    "db:setup:dev": "prisma generate --schema=prisma/schema.sqlite.prisma && prisma db push --schema=prisma/schema.sqlite.prisma",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.59.0",
    "clsx": "^2.1.1",
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.18",
    "postcss": "^8.4.47",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
